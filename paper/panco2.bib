@article{allen_cosmological_2011,
  title = {Cosmological {{Parameters}} from {{Observations}} of {{Galaxy Clusters}}},
  author = {Allen, Steven W. and Evrard, August E. and Mantz, Adam B.},
  year = {2011},
  month = aug,
  journal = {Annu. Rev. Astron. Astrophys.},
  volume = {49},
  number = {1},
  pages = {409--470},
  issn = {0066-4146},
  doi = {10.1146/annurev-astro-081710-102514},
  abstract = {Studies of galaxy clusters have proved crucial in helping to establish the standard model of cosmology, with a Universe dominated by dark matter and dark energy. A theoretical basis that describes clusters as massive, multicomponent, quasi-equilibrium systems is growing in its capability to interpret multiwavelength observations of expanding scope and sensitivity. We review current cosmological results, including contributions to fundamental physics, obtained from observations of galaxy clusters. These results are consistent with and complementary to those from other methods. We highlight several areas of opportunity for the next few years, and emphasize the need for accurate modeling of survey selection and sources of systematic error. Capitalizing on these opportunities will require a multiwavelength approach and the application of rigorous statistical frameworks, utilizing the combined strengths of observers, simulators, and theorists.},
  file = {/Users/fkeruzore/Zotero/storage/4IFGAJU7/Allen et al. - 2011 - Cosmological Parameters from Observations of Galax.pdf;/Users/fkeruzore/Zotero/storage/5DTIKLAK/annurev-astro-081710-102514.html}
}

@article{arnaud_universal_2010,
  title = {The Universal Galaxy Cluster Pressure Profile from a Representative Sample of Nearby Systems ({{REXCESS}}) and the {{YSZ}} \textendash{} {{M500}} Relation},
  author = {Arnaud, M. and Pratt, G. W. and Piffaretti, R. and B{\"o}hringer, H. and Croston, J. H. and Pointecouteau, E.},
  year = {2010},
  month = jul,
  journal = {A\&A},
  volume = {517},
  pages = {A92},
  issn = {0004-6361, 1432-0746},
  doi = {10.1051/0004-6361/200913416},
  abstract = {We investigate the regularity of cluster pressure profiles with REXCESS, a representative sample of 33 local (\emph{z{$<$}i/{$>$} {$<$} 0.2) clusters drawn from the REFLEX catalogue and observed with \emph{XMM-Newton{$<$}i/{$>$}. The sample spans a mass range of 10\textsuperscript{14{$<$}sup/{$>$} {$<$} \emph{M{$<$}i/{$>$}\textsubscript{500{$<$}sub/{$>$} {$<$} 10\textsuperscript{15{$<$}sup/{$>$} , where \emph{M{$<$}i/{$>$}\textsubscript{500{$<$}sub/{$>$} is the mass corresponding to a density contrast of 500. We derive an average profile from observations scaled by mass and redshift according to the standard self-similar model, and find that the dispersion about the mean is remarkably low, at less than 30 per cent beyond 0.2 \emph{R{$<$}i/{$>$}\textsubscript{500{$<$}sub/{$>$}, but increases towards the center. Deviations about the mean are related to both the mass and the thermo-dynamical state of the cluster. Morphologically disturbed systems have systematically shallower profiles while cooling core systems are more concentrated. The scaled profiles exhibit a residual mass dependence with a slope of \textasciitilde 0.12, consistent with that expected from the empirically-derived slope of the \emph{M{$<$}i/{$>$}\textsubscript{500{$<$}sub/{$>$} \textendash{} \emph{Y{$<$}i/{$>$}\textsubscript{X{$<$}sub/{$>$} relation; however, the departure from standard scaling decreases with radius and is consistent with zero at \emph{R{$<$}i/{$>$}\textsubscript{500{$<$}sub/{$>$}. The scatter in the core and departure from self-similar mass scaling is smaller compared to that of the entropy profiles, showing that the pressure is the quantity least affected by dynamical history and non-gravitational physics. Comparison with scaled data from several state of the art numerical simulations shows good agreement outside the core. Combining the observational data in the radial range [0.03\textendash 1] \emph{R{$<$}i/{$>$}\textsubscript{500{$<$}sub/{$>$} with simulation data in the radial range [1\textendash 4] \emph{R{$<$}i/{$>$}\textsubscript{500{$<$}sub/{$>$}, we derive a robust measure of the universal pressure profile, that, in an analytical form, defines the physical pressure profile of clusters as a function of mass and redshift up to the cluster ``boundary''. Using this profile and direct spherical integration of the observed pressure profiles, we estimate the integrated Compton parameter \emph{Y{$<$}i/{$>$} and investigate its scaling with \emph{M{$<$}i/{$>$}\textsubscript{500{$<$}sub/{$>$} and \emph{L{$<$}i/{$>$}\textsubscript{X{$<$}sub/{$>$}, the soft band X-ray luminosity. We consider both the spherically integrated quantity, \emph{Y{$<$}i/{$>$}\textsubscript{sph{$<$}sub/{$>$}(\emph{R{$<$}i/{$>$}), proportional to the gas thermal energy, and the cylindrically integrated quantity, \emph{Y{$<$}i/{$>$}\textsubscript{cyl{$<$}sub/{$>$}(\emph{R{$<$}i/{$>$})=\emph{Y{$<$}i/{$>$}\textsubscript{SZ{$<$}sub/{$>$} \emph{D{$<$}i/{$>$}\textsubscript{A{$<$}sub/{$>$}\textsuperscript{2{$<$}sup/{$>$}, which is directly related to the Sunyaev-Zel'dovich (SZ) effect signal. From the low scatter of the observed \emph{Y{$<$}i/{$>$}\textsubscript{sph{$<$}sub/{$>$}(\emph{R{$<$}i/{$>$}\textsubscript{500{$<$}sub/{$>$}) \textendash{} \emph{Y{$<$}i/{$>$}\textsubscript{X{$<$}sub/{$>$} relation we show that variations in pressure profile shape do not introduce extra scatter into the \emph{Y{$<$}i/{$>$}\textsubscript{sph{$<$}sub/{$>$}(\emph{R{$<$}i/{$>$}\textsubscript{500{$<$}sub/{$>$}) \textendash{} \emph{M{$<$}i/{$>$}\textsubscript{500{$<$}sub/{$>$} relation as compared to that from the \emph{Y{$<$}i/{$>$}\textsubscript{X{$<$}sub/{$>$} \textendash{} \emph{M{$<$}i/{$>$}\textsubscript{500{$<$}sub/{$>$} relation. The \emph{Y{$<$}i/{$>$}\textsubscript{sph{$<$}sub/{$>$}(\emph{R{$<$}i/{$>$}\textsubscript{500{$<$}sub/{$>$}) \textendash{} \emph{M{$<$}i/{$>$}\textsubscript{500{$<$}sub/{$>$} and \emph{Y{$<$}i/{$>$}\textsubscript{sph{$<$}sub/{$>$}(\emph{R{$<$}i/{$>$}\textsubscript{500{$<$}sub/{$>$}) \textendash{} \emph{L{$<$}i/{$>$}\textsubscript{X{$<$}sub/{$>$} relations derived from the data are in excellent agreement with those expected from the universal profile. This profile is used to derive the expected \emph{Y{$<$}i/{$>$}\textsubscript{SZ{$<$}sub/{$>$} \textendash{} \emph{M{$<$}i/{$>$}\textsubscript{500{$<$}sub/{$>$} and \emph{Y{$<$}i/{$>$}\textsubscript{SZ{$<$}sub/{$>$} \textendash{} \emph{L{$<$}i/{$>$}\textsubscript{X{$<$}sub/{$>$} relations for any aperture.}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}},
  copyright = {\textcopyright{} ESO, 2010},
  langid = {english},
  file = {/Users/fkeruzore/Zotero/storage/GYVGTWP6/Arnaud et al. - 2010 - The universal galaxy cluster pressure profile from.pdf;/Users/fkeruzore/Zotero/storage/GMSF4B6N/aa13416-09.html}
}

@article{artis_psz2_2022,
  title = {{{PSZ2 G091}}: {{A}} Massive Double Cluster at z   0.822 Observed by the {{NIKA2}} Camera},
  shorttitle = {{{PSZ2 G091}}},
  author = {Artis, E. and Adam, R. and Ade, P. and Ajeddig, H. and Andr{\'e}, P. and Arnaud, M. and Aussel, H. and Bartalucci, I. and Beelen, A. and Beno{\^i}t, A. and Berta, S. and Bing, L. and Bourrion, O. and Calvo, M. and Catalano, A. and De Petris, M. and D{\'e}sert, F. -X. and Doyle, S. and Driessen, E. F. C. and Ferragamo, A. and Gomez, A. and Goupy, J. and K{\'e}ruzor{\'e}, F. and Kramer, C. and Ladjelate, B. and Lagache, G. and Leclercq, S. and Lestrade, J. -F. and {Mac{\'i}as-P{\'e}rez}, J. -F. and Maury, A. and Mauskopf, P. and Mayet, F. and Monfardini, A. and {Mu{\~n}oz-Echeverr{\'i}a}, M. and Paliwal, A. and Perotto, L. and Pisano, G. and Pointecouteau, E. and Ponthieu, N. and Pratt, G. W. and Rev{\'e}ret, V. and Rigby, A. J. and Ritacco, A. and Romero, C. and Roussel, H. and Ruppin, F. and Schuster, K. and Shu, S. and Sievers, A. and Tucker, C. and Yepes, G.},
  year = {2022},
  month = jul,
  volume = {257},
  pages = {00003},
  address = {{eprint: arXiv:2111.05082}},
  doi = {10.1051/epjconf/202225700003},
  abstract = {PSZ2 G091.83+26.11 is a massive galaxy cluster with M500 = 7:43 \texttimes{} 1014M{$\odot$} at z = 0:822. This object exhibits a complex morphology with a clear bimodality observed in X-rays. However, it was detected and analysed in the Planck sample as a single, spherical cluster following a universal profile [1]. This model can lead to miscalculations of thermodynamical quantities, like the pressure profile. As future multiwavelength cluster experiments will detect more and more objects at higher redshifts (where we expect the fraction of merging objects to be higher), it is crucial to quantify this systematic effect. In this work, we use high-resolution observations of PSZ2 G091.83+26.11 by the NIKA2 camera to integrate the morphological characteristics of the cluster in our modelling. This is achieved by fitting a two-halo model to the SZ image and then by reconstruction of the resulting projected pressure profile. We then compare these results with the spherical assumption.},
  keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics},
  annotation = {ADS Bibcode: 2022EPJWC.25700003A},
  file = {/Users/fkeruzore/Zotero/storage/P7SQR5BV/Artis et al. - 2022 - PSZ2 G091 A massive double cluster at z   0.822 o.pdf}
}

@article{battaglia_cluster_2012-1,
  title = {On the {{Cluster Physics}} of {{Sunyaev-Zel}}'dovich and {{X-Ray Surveys}}. {{II}}. {{Deconstructing}} the {{Thermal SZ Power Spectrum}}},
  author = {Battaglia, N. and Bond, J. R. and Pfrommer, C. and Sievers, J. L.},
  year = {2012},
  month = oct,
  journal = {The Astrophysical Journal},
  volume = {758},
  pages = {75},
  issn = {0004-637X},
  doi = {10.1088/0004-637X/758/2/75},
  abstract = {Secondary anisotropies in the cosmic microwave background are a treasure-trove of cosmological information. Interpreting current experiments probing them are limited by theoretical uncertainties rather than by measurement errors. Here we focus on the secondary anisotropies resulting from the thermal Sunyaev-Zel'dovich (tSZ) effect; the amplitude of which depends critically on the average thermal pressure profile of galaxy groups and clusters. To this end, we use a suite of hydrodynamical TreePM-SPH simulations that include radiative cooling, star formation, supernova feedback, and energetic feedback from active galactic nuclei. We examine in detail how the pressure profile depends on cluster radius, mass, and redshift and provide an empirical fitting function. We employ three different approaches for calculating the tSZ power spectrum: an analytical approach that uses our pressure profile fit, a semianalytical method of pasting our pressure fit onto simulated clusters, and a direct numerical integration of our simulated volumes. We demonstrate that the detailed structure of the intracluster medium and cosmic web affect the tSZ power spectrum. In particular, the substructure and asphericity of clusters increase the tSZ power spectrum by 10\%-20\% at l \textasciitilde{} 2000-8000, with most of the additional power being contributed by substructures. The contributions to the power spectrum from radii larger than R 500 is \textasciitilde 20\% at l = 3000, thus clusters interiors (r {$<$} R 500) dominate the power spectrum amplitude at these angular scales.},
  keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics,cosmic background radiation,cosmology: theory,galaxies: clusters: general,large-scale structure of universe,methods: numerical},
  annotation = {ADS Bibcode: 2012ApJ...758...75B},
  file = {/Users/fkeruzore/Zotero/storage/WAFMRD3G/Battaglia et al. - 2012 - On the Cluster Physics of Sunyaev-Zel'dovich and X.pdf}
}

@article{benson_spt-3g_2014,
